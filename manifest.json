{
    "description": "Removes News Feed from Facebook.",  
    "manifest_version": 2,  
    "name": "Disable Facebook News Feed",  
    "version": "1.6", 
    "author": "Ankit Kumar", 
    "content_scripts":  
    [  
        {
            "css": ["style.css"],
            "matches": ["*://*.facebook.com/*"],
            "run_at": "document_start" 
        }  
    ]
}
